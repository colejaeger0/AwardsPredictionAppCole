{
  "name": "awardspredictionapp",
  "version": "0.0.2",
  "description": "All-In-One RN Template for Indie Hackers",
  "private": false,
  "scripts": {
    "typescript": "tsc --noEmit --skipLibCheck",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:new": "export NODE_OPTIONS=--max-old-space-size=8192 && react-native run-ios --simulator=\"iPhone 14\"",
    "free-up-memory": "node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js",
    "start": "react-native start",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "lint:old": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.js' --bundle-output='./ios/awardspredictionapp/main.jsbundle' --assets-dest='./ios/awardspredictionapp' --dev=false --platform='ios'",
    "bundle-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.android.js' --bundle-output='./android/app/src/main/assets/main.jsbundle' --assets-dest='./android/app/src/main/res/' --dev=false --platform='android'",
    "clean": "react-native-clean-project",
    "postinstall": "patch-package"
  },
